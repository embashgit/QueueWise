{
  "extends": ["next", "next/core-web-vitals", "prettier"],
  "env": {
    "jest": true,
    "es6": true
  },
  "plugins": ["prettier"],
  "rules": {
    "no-console": "warn",
    "no-use-before-define": "error",
    "no-unused-vars": [
      1,
      {
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ],
    "newline-before-return": "error"
  }
}
